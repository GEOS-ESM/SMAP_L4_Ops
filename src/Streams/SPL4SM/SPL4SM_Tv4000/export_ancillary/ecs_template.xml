        <CollectionMetaData>
            <ShortName>@SHORT_NAME@</ShortName>
            <VersionID>1</VersionID>
        </CollectionMetaData>
        <ECSDataGranule>
            <LocalGranuleID>@LocalGranuleID@</LocalGranuleID>
            <ProductionDateTime>@ProductionDateTime@</ProductionDateTime>
