!met_tag             = 'd5124_rpit3_jan12'                ! updated through present
!met_tag             = 'cross_d5124_RPFPIT'               ! uses "late-look" through present

! FP
!met_tag             = 'e5110_fp'
!met_tag             = 'e5130_fp'
!met_tag             = 'e5131_fp'
!met_tag             = 'cross_FP'

! FP with precip corrections as in pre-beta SMAP L4_SM products
