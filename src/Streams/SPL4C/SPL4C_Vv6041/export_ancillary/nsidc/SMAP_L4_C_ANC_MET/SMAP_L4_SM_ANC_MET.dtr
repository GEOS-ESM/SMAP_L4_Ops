TEMPLATE_RANGE_ENDING = 86399;

OBJECT = FILE_GROUP;

  DATA_TYPE = <PDR_SHORT_NAME>;
  DATA_VERSION = <L4_C_ECSVersionID>;
  NODE_NAME = <NODE_NAME>;

  OBJECT = FILE_SPEC;
    DIRECTORY_ID = <L4_C_INPUT_DIR>;
    FILE_ID = L4_C_met_%Y%m%dT000000_<L4_C_VersionID>_<PRODUCT_COUNTER>.h5;
    FILE_TYPE = SCIENCE;
    FILE_SIZE =
    FILE_CKSUM_TYPE = MD5;
    FILE_CKSUM_VALUE =
    FILE_LOCAL_DIR = <L4_C_OUTPUT_DIR>;
