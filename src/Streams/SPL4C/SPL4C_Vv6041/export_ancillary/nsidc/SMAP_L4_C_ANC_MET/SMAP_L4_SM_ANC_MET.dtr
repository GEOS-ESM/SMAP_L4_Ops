PDR_NAME       = SMAP_L4_C_ANC_MET.%Y%m%dT%H%M00.<SYSTEM_TIME>.PDR;
PDR_SHORT_NAME = SMAP_L4_C_ANC_MET;
PDR_LONG_NAME  = Soil Moisture Active Passive (SMAP) L4 Carbon Ancillary Meteorology Preprocessor Run Time Input Parameters;

CLOCK_METHOD   = iso_time;
MIN_FILE_COUNT = 1;
WAIT_TIME      = 259200;

ORIGINATING_SYSTEM = <ORIGINATING_SYSTEM>;
TOTAL_FILE_COUNT = 2;

#L4_C_INPUT_DIR = <L4_DATA_directoryID>/L4_C/<L4_C_VersionID>/met/Y%Y/M%m;
L4_C_INPUT_DIR = /css/smapl4/public/L4_Products/L4_C/<L4_C_VersionID>/met/Y%Y/M%m;
L4_C_OUTPUT_DIR = <L4_EXPORT_directoryID>/smapnsid/data_out/<CompositeStreamID>/data;

