    FILE_CLIENT_DIR = <CLIENT_DIR>;
  END_OBJECT = FILE_SPEC;

  OBJECT = FILE_SPEC;
    DIRECTORY_ID = <TEMPLATE_DIR>;
    FILE_ID = <L4_C_STREAM_version>_SMAP_L4_C.MET.%Y%m%d.xml;
    TEMPLATE_ID = <TEMPLATE_XML>;
    RANGE_BEGINNING = <TEMPLATE_RANGE_BEGINNING>;
    RANGE_ENDING = <TEMPLATE_RANGE_ENDING>;
    FILE_TYPE = METADATA;
    FILE_SIZE =
    FILE_CKSUM_TYPE = MD5;
    FILE_CKSUM_VALUE =
    FILE_LOCAL_DIR = <L4_C_OUTPUT_DIR>;
    FILE_CLIENT_DIR = <CLIENT_DIR>;
  END_OBJECT = FILE_SPEC;

END_OBJECT = FILE_GROUP;
