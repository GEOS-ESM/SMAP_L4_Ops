SPL3SMA_CompositeReleaseID  = h5dump -a Metadata/DatasetIdentification/CompositeReleaseID <SPL3SMA_FILE>
SPL4SMGP_CompositeReleaseID = h5dump -a Metadata/DatasetIdentification/CompositeReleaseID <SPL4SMGP_FILE>

L4_C_MOD_version      = h5dump -a L4C_MODIS_SOFTWARE_VERSION <MOD_FILE>
L4_C_MET_version      = h5dump -a L4C_MET_SOFTWARE_VERSION <MET_FILE>
