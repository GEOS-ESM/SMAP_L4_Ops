L4_C_GEOS5_version    = h5dump -a VersionID <GEOS5_FILE>
L4_C_SPL4CMDL_version = h5dump -a L4_C_SW_VERSION <SPL4CMDL_FILE>

L4_C_ANC_version          = <L4_C_ANC_version>
